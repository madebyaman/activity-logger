{
  "name": "activity-logger",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:test": "export NODE_ENV=\"test\" && next build",
    "start": "next start",
    "start:test": "NODE_ENV=\"test\" && next start",
    "lint": "next lint",
    "test": "jest --watch",
    "cache": "rm -rf .next",
    "migrate:local": "dotenv -e env.development.local -- npx prisma migrate dev",
    "reset:db": "dotenv -e .env.test.local -- npx prisma migrate reset",
    "push:local": "dotenv -e env.development.local -- npx prisma db push",
    "jest:cache": "jest --clear-cache",
    "cypress:open": "env-cmd -f .env.test.local cypress open",
    "cypress:start": "yarn reset:db && NODE_ENV=\"test\" start-server-and-test 'yarn run start:test' 3000 'yarn cypress:open'",
    "cypress:build": "yarn build:test && yarn cypress:start"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.6",
    "@heroicons/react": "^2.0.13",
    "@prisma/client": "^4.6.1",
    "@reach/visually-hidden": "^0.16.0",
    "@visx/group": "^2.10.0",
    "@visx/shape": "^2.12.2",
    "@visx/text": "^2.12.2",
    "axios": "^1.2.1",
    "bcrypt": "^5.0.1",
    "cookie": "^0.4.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "jsonwebtoken": "^8.5.1",
    "next": "^13.0.6",
    "next-router-mock": "^0.8.0",
    "nodemailer": "^6.8.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "17.0.12",
    "@types/nodemailer": "^6.4.6",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.38",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.2",
    "cypress": "^12.2.0",
    "dotenv-cli": "^6.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.7.0",
    "eslint-config-next": "^13.0.6",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "msw": "^0.49.2",
    "postcss": "^8.4.5",
    "prisma": "^4.6.1",
    "start-server-and-test": "^1.15.2",
    "tailwindcss": "^3.0.16",
    "ts-node": "^10.4.0",
    "typescript": "4.5.5"
  }
}
